{
	"manifest_version": 2,
	"name": "Fingerpop",
	"description": "Pertti Jarla's comic strip \"Fingerpori\" in a popup view.",
	"version": "0.6",
	"browser_action": {
		"default_icon": "img/icon19.png",
		"default_title": "Fingerpop",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"permissions": [
		"tabs",
		"http://www.hs.fi/*"
	]
}